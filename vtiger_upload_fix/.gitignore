# Vtiger CRM .gitignore

# Configuration files with sensitive data
config.inc.php
config.inc.php.backup
config.php
config.db.php
.env
.env.local
.env.*.local

# Cache directories
cache/
storage/cache/
templates_c/
parent_tabdata.php

# Log files
*.log
logs/
*.log.*

# Runtime/generated files
storage/202*/
storage/vtiger.txt
test/

# Docker runtime (if any)
docker-compose.override.yml
.docker/

# OS files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
*~

# IDE files
.idea/
.vscode/
*.swp
*.swo
*.swn
.project
.classpath
.settings/

# Backup files
*.bak
*.backup
*.old
*.orig
*.tmp

# Database dumps (if large)
*.sql.gz
*.sql.zip
backup/db/*.sql
!backup/db/.gitkeep

# Production package (if generated)
vtiger-prod/
vtiger-prod.zip
vtiger-prod.tar.gz

# Node modules (if any)
node_modules/
npm-debug.log
yarn-error.log

# Composer (if used)
vendor/
composer.lock

# Temporary files
*.tmp
*.temp
temp/
tmp/

# User uploads (if large)
storage/202*/*.jpg
storage/202*/*.png
storage/202*/*.pdf
storage/202*/*.doc
storage/202*/*.docx

# Compiled templates
templates_c/**/*.php
